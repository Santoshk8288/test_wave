body {    
    background-color: #F5F5F5;
	font-family:'DroidSans-reg';
	&::-webkit-scrollbar{
	  width: 6px;
	  background-color: #F5F5F5;
	  }
	&::-webkit-scrollbar-thumb{
	  background-color: #000000;
	  }
}
$white:#fff;
$red:#d5300e;
$green:#58a61b;
$dark_green:#35690e;
$purple:#704dab;
$dark_blue:#35497b;
$light_blue:#83beff;
$font-family-Reg:DroidSans-Reg;
$font-family-bold:DroidSans-bold;
$light_gray:#757575;
$dark_gray:#535353;

@font-face {
    font-family: DroidSans-Reg;
    src: url(assets/fonts/DroidSans.ttf);
}
@font-face {
    font-family: DroidSans-bold;
    src: url(assets/fonts/DroidSans-Bold.ttf);
}
@font-face {
    font-family: Lobster;
    src: url(assets/fonts/Lobster_1.3.otf);
}
@font-face {
  font-family: 'icomoon';
  src: url('assets/fonts/icomoon.eot');
  src: url('assets/fonts/icomoon.eot') format("embedded-opentype"), url('assets/fonts/icomoon.ttf') format("truetype"), url('assets/fonts/icomoon.woff') format("woff"), url('assets/fonts/icomoon.svg') format("svg");
  font-weight: normal;
  font-style: normal;
}


.navbar-brand{
	width: 200px;
    background: #ffffff;
}
.navbar-inverse .navbar-brand:focus, .navbar-inverse .navbar-brand:hover {
		color: $light_gray;
		background-color:#ffffff;
	}
.navbar-inverse .navbar-brand {
	color: $dark_gray;
	font-family:Lobster;
	font-size: 27px;
	letter-spacing: 2.5px;
}
.navbar-inverse .navbar-nav>li>a {
    color: #888888;	
	background: #e3e3e4;
}
.navbar-inverse .navbar-nav>li>a:focus, .navbar-inverse .navbar-nav>li>a:hover {
    color: $white;
    background-color: $dark_gray;
}
.navbar-inverse {
    background-color: #f5f5f5;
    border-color: #f5f5f5;
    box-shadow: 0 10px 30px -12px rgba(0, 0, 0, 0.42), 0 4px 25px 0px rgba(0, 0, 0, 0.12), 0 8px 10px -5px rgba(0, 0, 0, 0.2);
}
.navbar-inverse .navbar-nav>.active>a, .navbar-inverse .navbar-nav>.active>a:focus, .navbar-inverse .navbar-nav>.active>a:hover {
    color: #fff;
    background-color:  $light_gray;
}
.navbar-inverse .navbar-nav>.open>a, .navbar-inverse .navbar-nav>.open>a:focus, .navbar-inverse .navbar-nav>.open>a:hover {
    color: #fff;
    background-color:  $light_gray;		
}
.dropdown{	
	.caret {
		margin-left: 4px;
		vertical-align: middle;
		border-top: 7px dashed;
		border-top: 4px solid\9;
		border-right: 6px solid transparent;
		border-left: 6px solid transparent;
	}
	.dropdown-menu{	
		padding: 0px 0;
	}
}
.dropdown-menu>li>a {
		display: block;
		padding: 6px 20px;		
}
.dropdown-menu>li>a:focus, .dropdown-menu>li>a:hover {
    color: #ffffff;
    text-decoration: none;
    background-color: $light_gray; 
}
.panel{
    margin-top: 2%;
    margin-bottom: 1%;
	
	.panel-heading {
		padding: 15px;
		border-radius: 4px;
		font-weight: 700;
		color: white;
		font-family: $font-family-bold;
		margin-left: 15px;
		margin-right: 15px;
		margin-top: -25px;
		background: linear-gradient(60deg, #2196F3, #2196F3);
		box-shadow: 0 12px 20px -10px rgba(33, 150, 243, 0.28), 0 4px 20px 0px rgba(0, 0, 0, 0.12), 0 7px 8px -5px rgba(33, 150, 243, 0.2);
		border: 1px solid #2196F3;
	}
	.panel-body {
		padding: 20px;
		

		.table{
			margin-bottom: 0;
		}
		.table>tbody>tr>td, .table>tbody>tr>th, .table>tfoot>tr>td, .table>tfoot>tr>th, .table>thead>tr>td, .table>thead>tr>th {
			padding:11px;
			vertical-align: baseline;
		}
	}
	.table-bordered
		{
		     border: 1px solid #ddd;
			 margin-top:15px;
		}
	.table-responsive{
		.table-bordered
		{
		     border: 1px solid #ddd;
			 margin-top:15px;
		}
	}
	@media (max-width: 1366px){
	
		.panel-heading {
			padding: 12px;
			margin-left: 10px;
			margin-right: 10px;
		}
	}
	
}
.Reg_btn{
    border-radius: 2px;
	color: #fff;
	background-color:#35497b;
	border-color: #35497b;
	font-weight: 600;
	letter-spacing: 0.5px;
	height: 40px;
	}
.Reg_btn:hover,
.Reg_btn:focus,.Reg_btn:active:focus {
  background:#83beff;
  color:#35497b;
  }
  
.Danger_btn	{
    border-radius: 2px;
	color: #fff;
	background-color:#d5300e;
	border-color: #d5300e;
	font-weight: 600;
	letter-spacing: 0.5px;  
}

.forgot-wrapper,.Change-Pswd-wrapper{
  .Danger_btn {
    width:49%;
  }
}
.Danger_btn:hover,.Danger_btn:focus {
		box-shadow: 0 12px 20px -10px rgba(201, 48, 44, 0.28), 0 4px 20px 0px rgba(0, 0, 0, 0.12), 0 7px 8px -5px rgba(201, 48, 44, 1);
	}
	
.btn-light_green{
    border-radius: 2px;
    color: #fff;
    background-color: $green;
    border-color: $green;
    letter-spacing: 0.5px;
	 color: $white;
}
.btn-light_green:hover,.btn-light_green:focus,.btn-light_green:active:focus{
	background-color: $dark_green;
	color: $white;
    box-shadow: 0 12px 20px -10px rgba(53, 105, 14, 0.28), 0 4px 20px 0px rgba(0, 0, 0, 0.12), 0 7px 8px -5px rgba(53, 105, 14, 0.5);
}
.btn-light_blue{
    border-radius: 2px;
    color: #fff;
    background-color: #83beff;
    border-color: #35497b;
    letter-spacing: 0.5px;
	color: #161a23;
}
.btn-primary:hover,.btn-primary:focus,.btn-primary:active:focus {
    box-shadow: 0 12px 20px -10px rgba(40, 96, 144, 0.28), 0 4px 20px 0px rgba(0, 0, 0, 0.12), 0 7px 8px -5px rgba(40, 96, 144, 0.5);
}
.commanHeader{
	color: #535353;
	background-color: #426094;
	line-height: 3;
	margin-top: 0px;
	padding: 0px 20px;
	height: auto;
	font-family: $font-family-bold;	
	background-color: #f5f5f5;
    border-color: #f5f5f5;
    box-shadow: 0 10px 30px -12px rgba(0, 0, 0, 0.42), 0 4px 25px 0px rgba(0, 0, 0, 0.12), 0 8px 10px -5px rgba(0, 0, 0, 0.2);
}
.instructionList {
    margin-bottom: 15px;
}
.form-group{
	margin-bottom: 30px;    
	input{
	  border-radius: 0;
	  border: none;
	  box-shadow: unset;
	  border-bottom: 1px solid;
	  }
	label{
	  letter-spacing: 0.5px;
	  font-weight: 500;
		 }
	.help-block {
		color: #f92a2a;
		position: absolute;
	}
}

.modal-header {
    background: linear-gradient(60deg, #2196F3, #2196F3);
    box-shadow: 0 12px 20px -10px rgba(33, 150, 243, 0.28), 0 4px 20px 0px rgba(0, 0, 0, 0.12), 0 7px 8px -5px rgba(33, 150, 243, 0.2);
    border: unset;
    color: white;
}
.modal-body{
	.form-group{
		margin-bottom: 15px;
    img{
      width:100%;
      max-width:380px;
      overflow:hidden;
    }
    p{
        word-break: break-all;
        margin-bottom: 0;
    }
	}
}

@media (min-width: 768px){
	.navbar {
      border-radius: 0;
      position: fixed;
      top: 0;
      width: 100%;
      z-index:999;
  }
  .mainCls,.body_top_padding {
      padding-top: 70px;
  }

}
.Test-panel{
	.panel-body{
		.btn-light_blue {
			padding: 3px 12px;
			border-radius: 2px;
		}
	}
}

.card {
  display: inline-block;
  position: relative;
  width: 100%;
  margin: 25px 0;
  box-shadow: 0 1px 4px 0 rgba(0, 0, 0, 0.14);
  border-radius: 3px;
  color: rgba(0, 0, 0, 0.87);
  background: #fff;
  .card-height-indicator {
    margin-top: 100%;
  }
  &.row-space .header {
    padding: 15px 20px 0;
  }
  .title {
    margin-top: 0;
    margin-bottom: 5px;
  }
  .card-image {
    height: 60%;
    position: relative;
    overflow: hidden;
    margin-left: 15px;
    margin-right: 15px;
    margin-top: -30px;
    border-radius: 6px;
    img {
      width: 100%;
      height: 100%;
      border-radius: 6px;
      pointer-events: none;
    }
    .card-title {
      position: absolute;
      bottom: 15px;
      left: 15px;
      color: #fff;
      font-size: 1.3em;
      text-shadow: 0 2px 5px rgba(33, 33, 33, 0.5);
    }
  }
  .category:not([class*="text-"]) {
    color: #999999;
  }
  .card-content {
    padding: 15px 20px;
	
    .category {
		margin-bottom: 0;
		font-size: 22px;
		padding-bottom: 20px;
		padding-top: 10px;	
    }
  }
  .card-header {
    box-shadow: 0 10px 30px -12px rgba(0, 0, 0, 0.42), 0 4px 25px 0px rgba(0, 0, 0, 0.12), 0 8px 10px -5px rgba(0, 0, 0, 0.2);
    margin: -20px 0px 0 20px;
    border-radius: 3px;
    padding: 15px;
    background-color: #999999;
    .title {
      color: #FFFFFF;
    }
    .category {
      margin-bottom: 0;
      color: rgba(255, 255, 255, 0.62);
    }
    &.card-chart {
      padding: 0;
      min-height: 150px;
      + .content h4 {
        margin-top: 0;
      }
    }
    .ct-label {
      color: rgba(255, 255, 255, 0.7);
    }
    .ct-grid {
      stroke: rgba(255, 255, 255, 0.2);
    }
    .ct-series-a {
      .ct-point, .ct-line, .ct-bar, .ct-slice-donut {
        stroke: rgba(255, 255, 255, 0.8);
      }
      .ct-slice-pie, .ct-area {
        fill: rgba(255, 255, 255, 0.4);
      }
    }
  }
  .chart-title {
    position: absolute;
    top: 25px;
    width: 100%;
    text-align: center;
    h3 {
      margin: 0;
      color: #FFFFFF;
    }
    h6 {
      margin: 0;
      color: rgba(255, 255, 255, 0.4);
    }
  }
  .card-footer {
    margin: 0 20px 10px;
    padding-top: 10px;
    border-top: 1px solid #eeeeee;
    .content {
      display: block;
    }
    div {
      display: inline-block;
    }
    .author {
      color: #999999;
    }
    .stats {
      line-height: 22px;
      color: #999999;
      font-size: 12px;
      .glyphicon {
        position: relative;
        top: 4px;
        font-size: 16px;
      }
    }
    h6 {
      color: #999999;
    }
  }
  img {
    width: 100%;
    height: auto;
  }
  .category .glyphicon {
    position: relative;
    top: 6px;
    line-height: 0;
  }
  .category-social .fa {
    font-size: 24px;
    position: relative;
    margin-top: -4px;
    top: 2px;
    margin-right: 5px;
  }
  .author {
    .avatar {
      width: 30px;
      height: 30px;
      overflow: hidden;
      border-radius: 50%;
      margin-right: 5px;
    }
    a {
      color: #3C4858;
      text-decoration: none;
      .ripple-container {
        display: none;
      }
    }
  }
  .table {
    margin-bottom: 0;
    tr:first-child td {
      border-top: none;
    }
  }
  [data-background-color="purple"] {
    background: linear-gradient(60deg, #ab47bc, #8e24aa);
    box-shadow: 0 12px 20px -10px rgba(156, 39, 176, 0.28), 0 4px 20px 0px rgba(0, 0, 0, 0.12), 0 7px 8px -5px rgba(156, 39, 176, 0.2);
  }
  [data-background-color="blue"] {
    background: linear-gradient(60deg, #26c6da, #00acc1);
    box-shadow: 0 12px 20px -10px rgba(0, 188, 212, 0.28), 0 4px 20px 0px rgba(0, 0, 0, 0.12), 0 7px 8px -5px rgba(0, 188, 212, 0.2);
  }
  [data-background-color="green"] {
    background: linear-gradient(60deg, #66bb6a, #43a047);
    box-shadow: 0 12px 20px -10px rgba(76, 175, 80, 0.28), 0 4px 20px 0px rgba(0, 0, 0, 0.12), 0 7px 8px -5px rgba(76, 175, 80, 0.2);
  }
  [data-background-color="orange"] {
    background: linear-gradient(60deg, #ffa726, #fb8c00);
    box-shadow: 0 12px 20px -10px rgba(255, 152, 0, 0.28), 0 4px 20px 0px rgba(0, 0, 0, 0.12), 0 7px 8px -5px rgba(255, 152, 0, 0.2);
  }
  [data-background-color="red"] {
    background: linear-gradient(60deg, #ef5350, #e53935);
    box-shadow: 0 12px 20px -10px rgba(244, 67, 54, 0.28), 0 4px 20px 0px rgba(0, 0, 0, 0.12), 0 7px 8px -5px rgba(244, 67, 54, 0.2);
  }
  [data-background-color] {
    color: #FFFFFF;
    a {
      color: #FFFFFF;
    }
  }
  @media (max-width: 1366px){		
		.card-header {
			margin: -20px 0px 0 15px;
			padding: 10px;
		}
		.card-content{
			padding: 12px 20px 10px;
			.category {
				margin-bottom: 0;
				font-size: 22px;
				padding-top: 5px;
			}
		}		
	}
}
.chart-card{
	.category {
		padding-bottom: 0 !important;
	}
	.card-header {	
		margin: -20px 15px 0 15px;
	}
	@media (max-width: 1366px){		
		.card-header {
			margin: -20px 10px 0 10px;
		}
		
	}
}
.card-stats {
  .title {
    margin: 0;
	overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
  }
  .card-header {
    float: left;
    text-align: center;
    i {
      font-size: 36px;
      line-height: 56px;
      width: 56px;
      height: 56px;
    }
  }
  .card-content {
    text-align: right;
    padding-top: 10px;
  }
}

.card-nav-tabs {
  .header-raised {
    margin-top: -30px;
  }
  .nav-tabs {
    background: transparent;
    padding: 0;
  }
  .nav-tabs-title {
    float: left;
    padding: 10px 10px 10px 0;
    line-height: 24px;
  }
}

.card-plain {
  background: transparent;
  box-shadow: none;
  .card-header {
    margin-left: 0;
    margin-right: 0;
  }
  .content {
    padding-left: 5px;
    padding-right: 5px;
  }
  .card-image {
    margin: 0;
    border-radius: 3px;
    img {
      border-radius: 3px;
    }
  }
}

.iframe-container {
  margin: 0 -20px 0;
  iframe {
    width: 100%;
    height: 500px;
    border: 0;
    box-shadow: 0 10px 30px -12px rgba(0, 0, 0, 0.42), 0 4px 25px 0px rgba(0, 0, 0, 0.12), 0 8px 10px -5px rgba(0, 0, 0, 0.2);
  }
}

.card-profile, .card-testimonial {
  margin-top: 30px;
  text-align: center;
}

.card-profile .btn-just-icon.btn-raised, .card-testimonial .btn-just-icon.btn-raised {
  margin-left: 6px;
  margin-right: 6px;
}

.card-profile .card-avatar, .card-testimonial .card-avatar {
  max-width: 130px;
  max-height: 130px;
  margin: -50px auto 0;
  border-radius: 50%;
  overflow: hidden;
  box-shadow: 0 10px 30px -12px rgba(0, 0, 0, 0.42), 0 4px 25px 0px rgba(0, 0, 0, 0.12), 0 8px 10px -5px rgba(0, 0, 0, 0.2);
}

.card-profile .card-avatar + .content, .card-testimonial .card-avatar + .content {
  margin-top: 15px;
}

.card-profile.card-plain .card-avatar, .card-testimonial.card-plain .card-avatar {
  margin-top: 0;
}

