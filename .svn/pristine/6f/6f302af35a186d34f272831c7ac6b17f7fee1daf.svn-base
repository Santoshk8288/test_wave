/* slider image start*/

.sliderSection{
	background:#2196f3 ;
	background-size: cover;
	background-position: 100% 100%;                  
	position: relative;
	height:800px;
	overflow:hidden;
}
.sliderSection:after{
	content: "";
    display: block;
    background: #1d75ba;
    height: 25%;
    width: 111%;
    position: absolute;
    bottom: -111px;
    transform: rotate(183deg);
    left: -5px;
}
.sliderInner a {
	display: block;
	margin: 0 auto;
	text-align: center;
}

.leftImg {
	position: absolute;
	left: 0;
	right: auto;
	bottom: 0;
	width:30%;
	z-index:2;
}
.rightImg {
	position: absolute;
	left: auto;
	right: 0;
	bottom: 10px;
	width: 25%;
	z-index:2;
}

