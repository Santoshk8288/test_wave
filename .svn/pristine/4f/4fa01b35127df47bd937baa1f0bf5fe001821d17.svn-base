<nav class="navbar navbar-inverse" *ngIf="(authService.isLogin() && authService.getLoginUserData().usr_type != 1  && isCheckRoute())">
  	<div class="container-fluid" >
	    <div class="navbar-header">
	      	<a class="navbar-brand" (click)="goDashboard()" *ngIf="authService.getLoginUserData().usr_type != 3">TestWave</a>
	      	<a class="navbar-brand" *ngIf="authService.getLoginUserData().usr_type == 3">TestWave</a>
	    </div>
	    <ul class="nav navbar-nav ">
			<li class="header" *ngIf="authService.getLoginUserData().usr_type != 3" [ngClass]="{active:((tabName=='/historyResult' && settab == 'Dashboard') || (settab == 'Dashboard' && tabName == '/Dashboard') || tabName == '/Dashboard' ) }"><a (click)="goDashboard()">Dashboard</a></li> 
			<li class="header" [ngClass]="{active:tabName=='/test/testSeries'}"><a (click)="goTestseries()" >My Test</a></li>

			<li class="header" [ngClass]="{active:(settab=='history' && tabName=='/test/history' ) || (settab=='history' && tabName=='/historyResult' )}"><a (click)="goHistory()">History</a></li>
			
		</ul>
		<ul class="nav navbar-nav navbar-right">
			<li class="dropdown pull-right" [ngClass]="{active:tabName =='/test/profile' || tabName=='/changePassword' }">
				<a class="dropdown-toggle" data-toggle="dropdown" href="#"><i class="icon icon-user"></i>  <span class="text">{{authService.getLoginUserData().stu_fname === undefine ? authService.getLoginUserData().ins_name ==  undefine ? authService.getLoginUserData().gst_name : authService.getLoginUserData().ins_name : authService.getLoginUserData().stu_fname}}</span>
				<span class="caret"></span></a>
				<ul class="dropdown-menu">
				    <li *ngIf="authService.getLoginUserData().usr_type != 3"><a (click)="goProfile()">Profile</a></li>
					<li *ngIf="authService.getLoginUserData().usr_type != 3"><a (click)="changePass()">Change Password</a></li>
				  	<li *ngIf="authService.getLoginUserData().usr_type != 3"><a (click)="logOut()">Logout</a></li>
				  	<li *ngIf="authService.getLoginUserData().usr_type == 3"><a (click)="gestLogout()">Logout</a></li>
				</ul>
			</li>
		</ul>
  	</div>
</nav>
